<script defer='defer' type='text/javascript'> //<![CDATA[
// Memilih semua elemen dengan kelas "unduh"
var unduhLinks = document.querySelectorAll('.unduh');

unduhLinks.forEach(function(link) {
    // Mendapatkan nilai href dari setiap elemen
    var href = link.getAttribute('href');

    // Mengganti bagian URL yang diperlukan dengan ekspresi reguler
    var newHref = href.replace(/\/w[0-9]+\//, '/s0-d/');
    // Mengganti nilai href dengan yang baru
    link.setAttribute('href', newHref);

    // Menambahkan alert untuk konfirmasi download
    link.addEventListener('click', function(event) {
        event.preventDefault(); // Mencegah tindakan default link

        // Menampilkan pesan konfirmasi
        var confirmation = confirm("Apakah Anda ingin mengunduh wallpaper ini?");
        if (confirmation) {
            // Jika pengguna menekan OK, lanjutkan dengan mengarahkan ke URL unduhan baru
            window.location.href = link.getAttribute('href');
        }
    });

});


